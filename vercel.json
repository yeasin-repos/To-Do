{
     "builds": [{
          "src": "todo_list/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdasize": "15mb", "runtime": "python3.12" }
     }],
      "routes": [
          {
              "src": "/(.*)",
              "dest": "todo_list/wsgi.py"
          }
        ]
}
